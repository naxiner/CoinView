<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--GlobalItems-->
    <SolidColorBrush x:Key="Text" Color="#000000"/>
    <SolidColorBrush x:Key="Border" Color="#000000"/>
    <SolidColorBrush x:Key="BorderBG" Color="#E1E1E1"/>
    <SolidColorBrush x:Key="ButtonBG" Color="#D0D0D0"/>
    
    <!--MenuTab-->
    <SolidColorBrush x:Key="MenuBG" Color="#696969"/>
    <SolidColorBrush x:Key="MenuButtonBG" Color="#D0D0D0"/>

    <!--TopPanel-->
    <SolidColorBrush x:Key="TopPanel" Color="#929292"/>
    <SolidColorBrush x:Key="MainWindowBorder" Color="#D0D0D0"/>
    <SolidColorBrush x:Key="MainWindowBG" Color="#F1F1F1"/>
    <SolidColorBrush x:Key="TopPanelButtonBG" Color="#696969"/>

    <!--SearchWindow-->
    <SolidColorBrush x:Key="SearchBG" Color="LightGray"/>
    <SolidColorBrush x:Key="SearchBoxBG" Color="#F1F1F1"/>
    
</ResourceDictionary>