    <system:String x:Key="SearchByNameLabel">Suche nach titel (Bitcoin)</system:String>
    <system:String x:Key="SearchBySymbolLabel">Suche nach symbolen (BTC)</system:String>
    <system:String x:Key="CantFindLabel">Keine aufzeichnungen gefunden!</system:String>
