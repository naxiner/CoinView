<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
                    xmlns:system="clr-namespace:System;assembly=netstandard">
    
    <!--MenuTab-->
    <system:String x:Key="MenuHomeButton">MAIN</system:String>
    <system:String x:Key="MenuTopListButton">TOP 100</system:String>
    <system:String x:Key="MenuSearchButton">SUCHEN</system:String>
    <system:String x:Key="MenuSettingsButton">EINRICHTEN</system:String>
    
    <!--Global Items-->
    <system:String x:Key="DateTimeLable">Informationen aktualisiert ab:</system:String>
    <system:String x:Key="RefreshButton">Aktualisieren</system:String>

    <!--HomeWindow-->
    <system:String x:Key="HomeWindowLabel">MAIN</system:String>
    <system:String x:Key="TopFiveLable">Top 5 kryptowährungen</system:String>
    <system:String x:Key="PopupCopyLable">In die zwischenablage kopiert!</system:String>

    <!--TopListWindow-->
    <system:String x:Key="TopListWindowLabel">TOP 100 KRYPTOWÄHRUNGEN</system:String>
    <system:String x:Key="PriceLabel">Preis</system:String>
    <system:String x:Key="CurrencySupplyLabel">Menge auf dem markt</system:String>
