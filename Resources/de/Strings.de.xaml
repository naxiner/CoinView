    <system:String x:Key="OneMonthButton">1 m</system:String>
    <system:String x:Key="ReturnButton">Rückkehr</system:String>

    <!--SettingsWindow-->
    <system:String x:Key="SettingsLabel">EINRICHTEN</system:String>
    <system:String x:Key="ChangeLanguageLabel">Sprache ändern</system:String>
